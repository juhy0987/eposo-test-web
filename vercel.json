{
  "version": 2,
  "builds": [
    {
      "src": "eposo-test-api/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "eposo-test-web/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "eposo-test-api/dist/main.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
